#!/bin/bash

[ -d ~/.vim/pack/bundle/start ] || mkdir -p ~/.vim/pack/bundle/start
rm -rf ~/.vim/pack/bundle/start/*
cd ~/.vim/pack/bundle/start
git clone https://github.com/tpope/vim-surround
git clone https://github.com/tpope/vim-commentary
git clone https://github.com/tpope/vim-repeat
git clone https://github.com/tpope/vim-fugitive
git clone https://github.com/tpope/vim-abolish
git clone https://github.com/haya14busa/vim-operator-flashy
git clone https://github.com/haya14busa/incsearch.vim
git clone https://github.com/haya14busa/vim-asterisk
git clone https://github.com/chriskempson/base16-vim.git
git clone https://github.com/justinmk/vim-sneak.git
git clone https://github.com/Raimondi/delimitMate
git clone https://github.com/kana/vim-operator-user
git clone https://github.com/bootleq/vim-cycle
git clone https://github.com/KeyboardFire/vim-minisnip
git clone https://github.com/akhaku/vim-java-unused-imports
git clone https://github.com/jvenant/vim-java-imports
git clone https://github.com/tyru/skk.vim
git clone https://github.com/junegunn/fzf.git
git clone https://github.com/hauleth/asyncdo.vim
git clone https://github.com/einars/translit.vim
git clone https://github.com/udalov/kotlin-vim
git clone https://github.com/kazufukurou/vim-android-sensible
