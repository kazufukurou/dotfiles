alt + s
    pkill -USR1 -x sxhkd

alt + v
    term vedit

alt + n
    term

alt + b
    vimb

alt + l
    slock & sleep 1 && xset dpms force off

alt + semicolon
    dmenu_run -fn monospace:pixelsize=18 -nf "#999999" -nb "#555555" -sf "#ffffff" -sb "#555555"

XF86Audio{Raise,Lower}Volume
    amixer set Master playback {1+,1-}

XF86AudioMute
    amixer set Master toggle

XF86Audio{Next,Prev,Play}
    mplayercmd {next,prev,pause}
